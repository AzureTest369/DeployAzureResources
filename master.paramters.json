{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "keyVaultTemplateUri": {
      "value": "https://raw.githubusercontent.com/AzureTest369/DeployAzureResources/main/path/to/your/keyvault-template.json"
    },
    "webAppTemplateUri": {
      "value": "https://raw.githubusercontent.com/AzureTest369/DeployAzureResources/main/path/to/your/webapp-template.json"
    },
    "location": {
      "value": "eastus"
    },
    "keyVaultParameters": {
      "value": {
        "vaultName": {
          "value": "myKeyVaultExample"
        },
        "location": {
          "value": "eastus"
        },
        "tenantId": {
          "value": "00000000-0000-0000-0000-000000000000"
        },
        "objectId": {
          "value": "11111111-1111-1111-1111-111111111111"
        },
        "skuName": {
          "value": "standard"
        }
      }
    },
    "webAppParameters": {
      "value": {
        "hostingPlanName": {
          "value": "myAppServicePlan"
        },
        "webAppName": {
          "value": "myWebAppExample"
        },
        "location": {
          "value": "eastus"
        },
        "skuName": {
          "value": "S1"
        },
        "skuTier": {
          "value": "Standard"
        },
        "skuCapacity": {
          "value": 1
        },
        "siteAppSettings": {
          "value": [
            {
              "name": "WEBSITE_RUN_FROM_PACKAGE",
              "value": "1"
            },
            {
              "name": "EXAMPLE_SETTING",
              "value": "example-value"
            }
          ]
        }
      }
    }
  }
}
